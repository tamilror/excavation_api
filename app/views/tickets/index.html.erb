<h1>Ticket List</h1>

<table>
  <thead>
    <tr>
      <th>Request Number</th>
      <th>Request Type</th>
      <th>Request Action</th>
    </tr>
  </thead>
  <tbody>
    <% @tickets.each do |ticket| %>
      <tr>
        <td><%= ticket.request_number %></td>
        <td><%= ticket.request_type %></td>
        <td><%= ticket.request_action %></td>
        <td><%= link_to 'View', ticket_path(ticket) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

